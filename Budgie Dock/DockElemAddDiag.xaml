<Window x:Class="DockElemAddDiag"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Add element to dock" Height="200" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False">
    <StackPanel>
        <ComboBox Name="sCB" Margin="40">
            <ComboBoxItem BorderThickness="0" Content="Separator" IsSelected="True" Tag="sep" />
            <ComboBoxItem Content="Right Dock Contents Start" Tag="rightstart" BorderThickness="0" />
        </ComboBox>
        <StackPanel>
            <Button Name="okbtn">Add</Button>
            <Button Name="cancBTN">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
